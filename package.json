{
  "name": "react-globe",
  "version": "3.2.0",
  "description": "Create beautiful and interactive React + ThreeJS globe visualizations with ease.",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "three",
    "threejs",
    "webgl",
    "visualization",
    "interactive",
    "globe",
    "earth"
  ],
  "author": "Chris Zhou <chrisrzhou@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisrzhou/react-globe.git"
  },
  "bugs": {
    "url": "https://github.com/chrisrzhou/react-globe/issues"
  },
  "homepage": "https://react-globe.netlify.com/",
  "scripts": {
    "build": "rollup -c",
    "build:docs": "yarn docz build",
    "clean": "rm -rf dist .docz",
    "dev": "yarn docz dev",
    "lint": "yarn tsc && eslint --ext .js,.jsx,.ts,.tsx src docs",
    "lint:fix": "yarn lint --fix",
    "prepare": "yarn clean && yarn build",
    "size": "bundlesize",
    "unused": "yarn no-unused-export src/*.ts* --exclude src/*.d.ts"
  },
  "dependencies": {
    "@types/d3-array": "^2.0.0",
    "@types/d3-scale": "^2.1.1",
    "d3-array": "^2.2.0",
    "d3-scale": "^3.0.0",
    "es6-tween": "^5.5.10",
    "react-cached-callback": "^2.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "three-glow-mesh": "^0.1.1",
    "three-orbitcontrols": "^2.108.1",
    "three.interaction": "^0.2.2",
    "tippy.js": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@emotion/core": "^10.0.10",
    "@types/react": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "bundlesize": "^0.18.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.1.0",
    "no-unused-export": "^1.9.0",
    "prettier": "^1.18.2",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "three": "^0.108.0",
    "tslib": "1.10.0",
    "typescript": "^3.6.3"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "1 MB"
    }
  ],
  "peerDependencies": {
    "react": ">= 16.8.0",
    "three": ">= 0.102.0"
  }
}
